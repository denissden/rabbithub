<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="rabbitmq.client" Version="6.2.4" />
  </ItemGroup>

  <PropertyGroup>
    <Version>0.4.0</Version>
    <PackageId>RabbitHub</PackageId>
    <Authors>denissden</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Description>Simple RabbitMQ client</Description>
    <Readme>https://raw.githubusercontent.com/denissden/rabbithub/main/README.md</Readme>
    <RepositoryUrl>https://github.com/denissden/rabbithub.git</RepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>
    <LangVersion>latest</LangVersion>
    <Tags>rabbitmq</Tags>
  </PropertyGroup>

</Project>
